#include <stdio.h>

// 정수를 입력받아 해당 크기의 배열을 생성후 1씩 증가하는 배열을 만들어라.
int main()
{
	int count = 1;
	int numbers[5][5];
	int size;

	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j < 5; j++)
		{
			numbers[i][j] = count;
			count++;
		}
	}

	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j < 5; j++)
		{
			printf("%3d", numbers[i][j]);
		}

		printf("\n");
	}
}
