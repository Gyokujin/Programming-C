#include <stdio.h>

int main()
{
	int year = 0;
	printf("년도를 입력하세요 : ");
	scanf_s("%d", &year);
	int a = ((year % 4 == 0) || (year % 100 == 0) || (year % 400 == 0));

	printf("%d\n", a);

	if (a == 1)
	{
		printf("%d년은 윤년입니다.\n", year);
	}
	else 
	{
		printf("%d년은 윤년이 아닙니다.\n", year);
	}

	int numA = 15;
	int numB = 25;
	int numC = 0;

	numC = numA > numB ? numA : numB;
	printf("%d\n", numC);
}
