#include <stdio.h>

int main()
{
	int x = 0;
	int y = 0;

	printf("첫번째 숫자를 입력하세요. ");
	scanf_s("%d", &x);

	printf("두번째 숫자를 입력하세요. ");
	scanf_s("%d", &y);

	printf("두 수의 합은 %d\n", x + y);
	printf("두 수의 차는 %d\n", x - y);
	printf("두 수의 곱은 %d\n", x * y);
	printf("두 수를 나눈 몫은 %d, 나머지는 %d\n", x / y, x % y);
	return 0;
}
