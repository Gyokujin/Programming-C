#include <iostream>
using namespace std;

// (A+B)%C는 ((A%C) + (B%C))%C 와 같을까? //(A×B) % C는((A% C) ×(B% C)) % C 와 같을까 ?
// 세 수 A, B, C가 주어졌을 때, 위의 네 가지 값을 구하는 프로그램을 작성하시오.
void main()
{
	int A, B, C;
	cin >> A >> B >> C;

	cout << (A + B) % C << endl;
	cout << ((A % C) + (B % C)) % C << endl;
	cout << (A * B) % C << endl;
	cout << ((A % C) * (B % C)) % C << endl;
}
